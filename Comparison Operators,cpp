#include <iostream>
using namespace std;
int main(){
    int x=5 ,y=7;
    cout<< "(x<y)" << (x<y) <<'\n';
    cout<< "(x>y)" << (x>y) <<'\n';
    cout<< "(x==y)" << (x==y) <<'\n';
    cout<< "(x!=y)" << (x!=y) <<'\n';
    cout<< "(x>=y)" << (x>=y) <<'\n';
    cout<< "(x<=y)" << (x<=y) <<'\n';
    return 0;
}

